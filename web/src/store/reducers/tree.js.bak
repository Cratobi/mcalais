import { TREE } from '..'

const initialState = {
    assets: [],
    liabilities: [],
    equities: [],
    expenses: [],
    incomes: [],
}

const tree = (state = initialState, { type, payload }) => {
  switch (type) {
    case TREE.REPLACE: {
      return payload
    }
    default:
      return state
  }
}

export default tree
